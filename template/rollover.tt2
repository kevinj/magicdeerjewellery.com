<script>var img = new Image; img.src="[% rollover %]";</script>

[% UNLESS image.nolink %]
<a name="[% image.name %]" class="navButton" href="[% href || '#' %]">
[% END %]

[% IF selected %]
<img src="images/[% image.name %]_hover.png" />
[% ELSE %]
<img src="images/[% image.name %].png"
     class="rollover"
     section="[% image.section %]"
     name="[% image.name %]"/>
[% END %]

[% UNLESS image.nolink %]
</a>
[% END %]
