<script>var img = new Image; img.src="images/[% file %]_hover.jpg";</script>
<a name="[% file %]" class="rollover" class="navButton" href="[% href || '#' %]">
[% IF selected %]
<img src="images/[% file %]_hover.jpg" />
[% ELSE %]
<img src="images/[% file %].jpg" onmouseover="this.src='images/[% file %]_hover.jpg'" onmouseout="this.src='images/[% file %].jpg'" />
[% END %]
</a>
