[% WRAPPER wrapper.tt2
    name="gallery"
    title="Gallery"
    imgs=[['gallery1_1','gallery1_2','gallery1_3','gallery1_4'],
          ['gallery1_5','gallery1_6','gallery1_7','gallery1_8']]
%]
<img class="target" src="images/gallery1_1_big.png"/>
<div style="display:none" class="caption">18 Karat Yellow Gold Handmade Chain</div>
<div class="pages">
    <a href="gallery1.html">Page 1</a>
    <a href="gallery2.html">Page 2</a>
    <a href="gallery3.html">Page 3</a>
</div>
<script>
jQuery(function() {
    jQuery('.rollover').click(function() {
        var $this = jQuery(this);
        var $img = $this.find('img');
        var url = "images/" + $this.attr('name') + "_big.png";
        jQuery('.target').fadeOut(function () {
            jQuery('.target').attr('src', url).fadeIn();
        });
    });
});
</script>
[% END %]
